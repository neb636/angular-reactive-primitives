{
  "name": "angular-reactive-creation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve reference-app",
    "build": "ng build reactive-primitives",
    "build:reference-app": "ng build reference-app",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint reference-app && ng lint reactive-primitives",
    "compile:docs": "node scripts/compile-docs.js",
    "generate:routes": "node scripts/generate-routes.js",
    "build:docs": "npm run compile:docs && npm run generate:routes",
    "docs:dev": "npm run build:docs && ng serve reference-app",
    "docs:build": "npm run build:docs && ng build reference-app --configuration=production"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "highlight.js": "^11.11.1",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.0",
    "@eslint/js": "^9.38.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash-es": "^4.17.12",
    "angular-eslint": "20.5.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "prettier": "3.6.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.1"
  }
}
