{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "reactive-primitives": {
      "projectType": "library",
      "root": "projects/reactive-primitives",
      "sourceRoot": "projects/reactive-primitives/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/reactive-primitives/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/reactive-primitives/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/reactive-primitives/tsconfig.spec.json"
          }
        }
      }
    },
    "reference-app": {
      "projectType": "application",
      "root": "projects/reference-app",
      "sourceRoot": "projects/reference-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "tsConfig": "projects/reference-app/tsconfig.app.json",
            "outputPath": "dist/reference-app",
            "index": "projects/reference-app/src/index.html",
            "browser": "projects/reference-app/src/main.ts",
            "assets": [
              "projects/reference-app/src/assets"
            ],
            "styles": [
              "projects/reference-app/src/styles.css"
            ],
            "inlineStyleLanguage": "css"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true
            },
            "development": {}
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "reference-app:build:production"
            },
            "development": {
              "buildTarget": "reference-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
