{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-reactive-primitives": {
      "projectType": "library",
      "root": "projects/angular-reactive-primitives",
      "sourceRoot": "projects/angular-reactive-primitives/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-reactive-primitives/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/angular-reactive-primitives/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/angular-reactive-primitives/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/angular-reactive-primitives/**/*.ts"]
          }
        }
      }
    },
    "documentation-site": {
      "projectType": "application",
      "root": "projects/documentation-site",
      "sourceRoot": "projects/documentation-site/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "tsConfig": "projects/documentation-site/tsconfig.app.json",
            "outputPath": "dist/documentation-site",
            "index": "projects/documentation-site/src/index.html",
            "browser": "projects/documentation-site/src/main.ts",
            "assets": ["projects/documentation-site/src/assets"],
            "styles": ["projects/documentation-site/src/styles.css"],
            "inlineStyleLanguage": "css"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true
            },
            "development": {}
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "documentation-site:build:production"
            },
            "development": {
              "buildTarget": "documentation-site:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/documentation-site/**/*.ts",
              "projects/documentation-site/**/*.html"
            ]
          }
        }
      }
    }
  }
}
